<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<GridPane id="root" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
          minWidth="-Infinity" prefHeight="800.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/8.0.171">
    <columnConstraints>
        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="50.0" valignment="CENTER" vgrow="NEVER"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="NEVER"/>
        <RowConstraints minHeight="10.0" prefHeight="130.0" vgrow="NEVER"/>
        <RowConstraints minHeight="10.0" prefHeight="150.0" vgrow="NEVER"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <stylesheets>
        <URL value="@../css/profile.css"/>
        <URL value="@../css/main.css"/>
    </stylesheets>
    <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" styleClass="default-button"
            text="Change Password" GridPane.columnIndex="1" GridPane.rowIndex="5" id="change-password-btn"/>
    <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" styleClass="default-button"
            text="Change Nickname" GridPane.columnIndex="1" GridPane.rowIndex="6" id="change-nickname-btn"/>
    <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" styleClass="default-button" text="Back"
            id="back-btn" GridPane.columnIndex="1" GridPane.rowIndex="7"/>
    <Label styleClass="title-label" text="Profile" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    <HBox id="profile-pic-container" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1"
          GridPane.rowIndex="3"/>
    <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" style="-fx-spacing: 10px;"
          GridPane.columnIndex="1" GridPane.rowIndex="4">
        <Label id="username-label" styleClass="default-label" text="Username: test"/>
        <Label id="nickname-label" styleClass="default-label" text="Nickname: test"/>
    </VBox>
</GridPane>
